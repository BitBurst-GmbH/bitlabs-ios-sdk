{
  "name": "BitLabs",
  "version": "3.1.12",
  "summary": "BitLabs - monetize your app with rewarded surveys.",
  "description": "BitLabs offers the opportunity to monetize your app with rewarded surveys easily.",
  "homepage": "https://github.com/BitBurst-GmbH/bitlabs-ios-sdk",
  "license": "Commercial",
  "authors": {
    "BitBurst GmbH": "bitlabs@bitburst.net"
  },
  "source": {
    "git": "https://github.com/BitBurst-GmbH/bitlabs-ios-sdk.git",
    "tag": "3.1.12"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "11.0"
  },
  "dependencies": {
    "Alamofire": [
      "~> 5.5"
    ]
  },
  "default_subspecs": "Core",
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "source_files": "BitLabs/Tests/Unit\\ Tests/*.swift",
      "dependencies": {
        "OHHTTPStubs/Swift": [

        ]
      }
    },
    {
      "name": "UITests",
      "test_type": "ui",
      "requires_app_host": true,
      "app_host_name": "BitLabs/AppHost",
      "dependencies": {
        "BitLabs/AppHost": [

        ]
      },
      "source_files": "BitLabs/Tests/UI\\ Tests/*.swift"
    }
  ],
  "appspecs": [
    {
      "name": "AppHost",
      "source_files": "BitLabs/AppHost/**/*.{swift}",
      "resources": "BitLabs/AppHost/**/*.{xib,storyboard}"
    }
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "BitLabs/Classes/{Shared,Core}/**/*.swift",
      "resources": [
        "BitLabs/Resources/{Shared,Core}/**/*.xib",
        "BitLabs/Localizations/**/*.strings",
        "BitLabs/Resources/{Shared,Core}/**/*.xcassets"
      ]
    },
    {
      "name": "Unity",
      "source_files": "BitLabs/Classes/{Shared,Unity}/**/*.swift",
      "resources": [
        "BitLabs/Resources/Shared/**/*.xib",
        "BitLabs/Localizations/**/*.strings",
        "BitLabs/Resources/Shared/**/*.xcassets"
      ]
    }
  ],
  "swift_version": "5.0"
}

{
  "name": "BitLabs",
  "version": "0.1.7",
  "summary": "BitLabs - monetize your app with rewarded surveys.",
  "description": "BitLabs offers the opportunity to monetize your app with rewarded surveys easily.",
  "homepage": "https://github.com/BitBurst-GmbH/bitlabs-ios-sdk",
  "license": "Commercial",
  "authors": {
    "BitBurst GmbH": "bitlabs@bitburst.net"
  },
  "source": {
    "git": "https://github.com/BitBurst-GmbH/bitlabs-ios-sdk.git",
    "tag": "0.1.7"
  },
  "default_subspecs": "Core",
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "BitLabs/Classes/**/*.swift",
  "resources": [
    "BitLabs/Classes/**/*.xib",
    "BitLabs/Localizations/**/*.strings"
  ],
  "swift_versions": "5.0",
  "dependencies": {
    "Alamofire": [
      "~> 5.5"
    ]
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "BitLabs/Classes/Shared/**/*.swift",
        "BitLabs/Classes/Core/*.swift"
      ],
      "resources": [
        "BitLabs/Classes/Shared/**/*.xib",
        "BitLabs/Localizations/**/*.strings"
      ]
    },
    {
      "name": "Unity",
      "source_files": [
        "BitLabs/Classes/Shared/**/*.swift",
        "BitLabs/Classes/Unity/*.swift"
      ],
      "resources": [
        "BitLabs/Classes/Shared/**/*.xib",
        "BitLabs/Localizations/**/*.strings"
      ]
    }
  ],
  "swift_version": "5.0"
}
